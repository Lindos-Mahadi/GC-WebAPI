    function CalcTotalFamilyMember() {
        var MaleFamilyMember = document.getElementById('MaleFamilyMember').value;
        var FemaleFamilyMember = document.getElementById('FemaleFamilyMember').value;
        if (MaleFamilyMember == '') {
            return;
        }
        if (FemaleFamilyMember == '') {
            //$.alert.open("Message", "saved");
            return;
        }
        var amount = parseFloat(MaleFamilyMember) + parseFloat(FemaleFamilyMember);
        document.getElementById('TotalFamilyMember').value = parseFloat(amount);
    }

    function CalcTotalExpense() {
        var HouseRent = document.getElementById('HouseRent').value;
        var FoodCost = document.getElementById('FoodCost').value;
        var EducationMedicalExpense = document.getElementById('EducationMedicalExpense').value;
        var OtherExpense = document.getElementById('OtherExpense').value;

        if (HouseRent == '') {
            return;
        }
        if (FoodCost == '') {
            return;
        }
        if (EducationMedicalExpense == '') {
            return;
        }
        if (OtherExpense == '') {
            return;
        }

        var amount = parseFloat(HouseRent) + parseFloat(FoodCost) + parseFloat(EducationMedicalExpense) + parseFloat(OtherExpense);
        document.getElementById('TotalExpense').value = parseFloat(amount);
    }



