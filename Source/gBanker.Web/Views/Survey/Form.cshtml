    function LoadCountry() {
        var ddlPerCountryID = $("#ddlPerCountryID");
        var ddlCountryID = $("#ddlCountryID");
        var ddlComCountryID = $("#ddlComCountryID");
        var ddlBizCountryID = $("#ddlBizCountryID");
     var ddlActuPerCountryID = $("#ddlActuPerCountryID");

        $.ajax({
            type: 'GET',
            contentType: "application/json; charset=utf-8",
            url: '@Url.Action("GetCountryList", "Survey", "http")',
            data: {},
            dataType: 'json',
            async: true,
            success: function (data) {
                ddlPerCountryID.html('');
                ddlCountryID.html('');
                ddlComCountryID.html('');
                ddlBizCountryID.html('');
    ddlActuPerCountryID.html('');

                //ddlHouseType.append($('<option></option>').val("0").html("Please Select"));
                $.each(data, function (id, option) {
                    ddlPerCountryID.append($('<option></option>').val(option.Value.trim()).html(option.Text));
                    ddlCountryID.append($('<option></option>').val(option.Value.trim()).html(option.Text));
                    ddlComCountryID.append($('<option></option>').val(option.Value.trim()).html(option.Text));
                    ddlBizCountryID.append($('<option></option>').val(option.Value.trim()).html(option.Text));
                    ddlActuPerCountryID.append($('<option></option>').val(option.Value.trim()).html(option.Text));

                });
            },
            error: function (request, status, error) {
                alert(request.statusText + "/" + request.statusText + "/" + error);
            }
        });

        $("#ddlPerCountryID option[value='" + 18 + "']").prop('selected', 'selected');
        $("#ddlCountryID option[value='" + 18 + "']").prop('selected', 'selected');

        $("#ddlComCountryID option[value='" + 18 + "']").prop('selected', 'selected');
        $("#ddlBizCountryID option[value='" + 18 + "']").prop('selected', 'selected');
        $("#ddlActuPerCountryID option[value='" + 18 + "']").prop('selected', 'selected');


    }


