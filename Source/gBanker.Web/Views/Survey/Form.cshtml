    function showNominiImage(fileInput) {
        var files = fileInput.files;
        for (var i = 0; i < files.length; i++) {
            var file = files[i];
            var imageType = /image.*/;

            if (file.type.match(imageType)) {
                //continue;
                var size = Math.round(file.size / 1024);
                if (size <= 10000) {
                    var img = document.getElementById("targetNomini");
                    img.file = file;
                    var reader = new FileReader();
                    reader.onload = (function (aImg) {
                        return function (e) {
                            aImg.src = e.target.result;

                            let targetNomini_imgsrc = aImg.src;

                            let targetNominiSplit  =  targetNomini_imgsrc.split(",");

                              let NomineePhoto         = targetNominiSplit[1] ;
                            $('#txtNomineePhotoBinary').val(NomineePhoto);

                        };
                    })(img);
                        reader.readAsDataURL(file);
                }
                else
                    alert('Image file cannot be greater than 100 KB.');
            }
            else {
                $("#targetNomini").replaceWith($("#targetNomini").clone(true));
                alert('Please select a valid image file');
            }

        }

    }




