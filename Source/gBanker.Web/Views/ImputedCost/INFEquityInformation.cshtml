            <div class="col-md-12">
                <div class="raw">
                    <div class="col-md-6">
                        <div class="form-group">
                            @Html.LabelFor(model => model.NPK_INF_ADJ, htmlAttributes: new { @class = "control-label required" })
                            @Html.TextBoxFor(model => model.NPK_INF_ADJ, new { @class = "form-control CheckNumeric", @autocomplete = "off" })
