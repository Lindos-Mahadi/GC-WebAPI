                        <div class="form-group">
                            @Html.LabelFor(model => model.PK_INF_ADJ, htmlAttributes: new { @class = "control-label required" })
                            @Html.TextBoxFor(model => model.PK_INF_ADJ, new { @class = "form-control CheckNumeric", @autocomplete = "off" })
                            @Html.ValidationMessageFor(model => model.PK_INF_ADJ)
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="raw">
            <div class="col-md-12">
                <div class="raw">
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="form-group form-action">
                                <button type="submit" class="btn btn-sm btn-primary"> Submit </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
