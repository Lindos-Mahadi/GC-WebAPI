@{
    ViewBag.Title = "InActiveMemberList";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}


<script type="text/javascript">
    $(document).ready(function () {


        $("#btnView").click(function (e) {
            e.preventDefault();

            var EmployeeStatus = $("#ddlQtype").val();

            var url = '/GroupwiseReport/GenerateInactiveMemberList';

            window.open(url, 'mywindow', 'fullscreen=yes, scrollbars=auto');
        });


        $("#btnDrop").click(function (e) {
            e.preventDefault();

            var EmployeeStatus = $("#ddlQtype").val();

            var url = '/GroupwiseReport/GenerateInactiveMemberListDrop';

            window.open(url, 'mywindow', 'fullscreen=yes, scrollbars=auto');
        });
    });
</script>



<h2 class="page-title">InActive Member List</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="">

        

        <h2 class="page-title"></h2>
        <div class="row">
            <div class="col-md-8">
                <div class="form-group from-action">
                    <input type="submit" value="View" class="btn btn-primary" id="btnView" />
                </div>
            </div>

            <div class="col-md-8" style="padding-right:20px;" align="right">
                <input id="btnDrop" type="submit" value="Dropable" class="btn btn-primary" />
            </div>

        </div>


    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryui")
}






