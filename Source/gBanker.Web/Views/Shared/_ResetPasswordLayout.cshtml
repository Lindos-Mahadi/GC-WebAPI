<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title - gBanker Web</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Charisma, a fully featured, responsive, HTML5, Bootstrap admin template.">
    <meta name="author" content="GB Team">
    <!-- The styles -->
    <link id="bs-css" href="~/Content/charisma-master/css/bootstrap-cerulean.min.css" rel="stylesheet">
    <link href="~/Content/charisma-master/css/charisma-app.css" rel="stylesheet">
    <link href='~/Content/charisma-master/bower_components/bootstrap-tour/build/css/bootstrap-tour.min.css' rel='stylesheet'>
    <link href='~/Content/charisma-master/css/animate.min.css' rel='stylesheet'>
    <link href='~/Content/charisma-master/css/custom.css' rel='stylesheet'>
    <!-- jQuery -->
    <script src="~/Content/charisma-master/bower_components/jquery/jquery.min.js"></script>
    <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <!-- The fav icon -->
   
    <link rel="shortcut icon" href="../images/LOGO_G.ico">
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="row">
                <div class="col-md-5 col-sm-8 col-xs-10 center login-header">
                    <img src="~/Content/images/gbankerPlusNew.png" alt="gBanker Web" width="200">
                </div>
                <!--/span-->
            </div><!--/row-->

            <div class="row">
                <div class="well col-md-5 col-sm-8 col-xs-10 center login-box">
                    <div class="alert alert-info">
                        Please enter your new password.
                    </div>
                    @RenderBody()
                </div>
                <!--/span-->
            </div><!--/row-->

            <div class="row">
                <div class="col-md-5 col-sm-8 col-xs-10 center">
                    Copyright &copy; <a href="http://grameencommunications.org/" target="_blank">Grameen Communications</a> @DateTime.Now.Year
                    <br />
                    <label id="lblKServiceMessage" style="font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif; color:red; font-size:30px;"></label>
                </div>
                <!--/span-->
            </div><!--/row-->


        </div><!--/fluid-row-->
    </div><!--/.fluid-container-->
    <!-- external javascript -->
    <script src="~/Content/charisma-master/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- application script for Charisma demo -->
    <script src="~/Content/charisma-master/js/charisma.js"></script>
</body>
</html>